source 'https://github.com/CocoaPods/Specs.git'
platform :ios, '8.0'

def myPods
pod 'MMDrawerController', '~> 0.5.7'
pod 'GoogleMaps'
pod 'iOS-KML-Framework'
pod 'SwiftHTTP'
pod 'JSONJoy-Swift'
pod 'SVProgressHUD'
end

target ‘Rivergreenway’ do
	myPods
end
target ‘RivergreenwayTests’ do
	myPods
end
target ‘RivergreenwayUITests’ do
	myPods
end
use_frameworks!
inhibit_all_warnings!
